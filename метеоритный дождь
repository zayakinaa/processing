x = 1000
a = 220
b = 120
c = 160
d = 280
e = 250
f = 150
def setup():
    size(450,600)
def  draw():
    #background(255)
    translate(-250,-250) 
    
    global a,b,c,d,e,f,x
    push()
    fill(255,206,10)
    triangle(a,b,c,a,d,a)
    triangle(a,e,c,f,d,f)
    pop()
    
    a += 1
    b += 1
    c += 1
    d += 1
    e += 1
    f += 1
    
    fill(0,255,136)
    ellipse(x,x,850,850)
    x -=0.9
